    "main": "app/main",
    "scripts": {
        "run.android.production": "cross-var tns run android --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASS --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_PASS --env.adhoc  --env.snapshot --env.compileSnapshot",
        "pandroid": "cross-var tns build android --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASSWORD --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.snapshot --env.compileSnapshot",
        "run.ios.production": "tns run ios --env.adhoc --release --for-device --provision 'match AdHoc com.lokavaluto.lokavaluto'"
    },
